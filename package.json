    "body-parser": "1.17.0",
    "cookie-parser": "1.4.3",
    "date-fns": "1.28.0",
    "dotenv": "4.0.0",
    "express": "4.15.0",
    "express-session": "1.15.1",
    "flexboxgrid": "6.3.1",
    "inferno": "^1.3.0-rc.10",
    "inferno-compat": "^1.3.0-rc.10",
    "inferno-redux": "^1.3.0-rc.10",
    "lodash": "4.17.4",
    "mongoose": "4.8.5",
    "mongoose-geojson-schema": "2.1.2",
